<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:angle="180" android:startColor="#2864FF" android:endColor="#195AFF" android:type="linear" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <gradient android:angle="180" android:startColor="#112863" android:endColor="#081C4E" android:type="linear" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:angle="180" android:startColor="#202D48" android:endColor="#081B41" android:type="linear" />
        </shape>
    </item>
</selector>
